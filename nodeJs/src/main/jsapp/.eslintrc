{
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "amd": true,
    "es6": true,
    "node": true,
    "mocha": true
  },
  rules: {
    'react/no-unused-prop-types': [0, {
      customValidators: [
      ],
      skipShapeProps: true,
    }],
  },
  "extends": ["airbnb"]
}
